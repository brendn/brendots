# opens iTerm2
cmd - return : osascript ~/.config/scripts/open_iterm2.scpt
cmd - 0 : open -na 'google chrome'
cmd - e : yabai -m space --toggle show-desktop
ctrl + alt + cmd - r : yabai --restart-service
cmd + alt - left : yabai -m window --warp west
cmd + alt - down : yabai -m window --warp south
cmd + alt - up : yabai -m window --warp north
cmd + alt - right : yabai -m window --warp east

lctrl + alt - left : yabai -m window --focus west
lctrl + alt - down : yabai -m window --focus south
lctrl + alt - up : yabai -m window --focus north
lctrl + alt - right : yabai -m window --focus east

lctrl + cmd  + alt - left 	: yabai -m window --resize left:-50:0; \
                  	  	  yabai -m window --resize right:-50:0
lctrl + cmd + alt - down 	: yabai -m window --resize bottom:0:50; \
                  		  yabai -m window --resize top:0:50
lctrl + cmd + alt - up		: yabai -m window --resize top:0:-50; \
                  		  yabai -m window --resize bottom:0:-50
lctrl + cmd + alt - right 	: yabai -m window --resize right:50:0; \
                  		  yabai -m window --resize left:50:0

# Float / Unfloat window
shift + alt - space : \
    yabai -m window --toggle float; \
    yabai -m window --toggle border

# Make window native fullscreen
alt - f         : yabai -m window --toggle zoom-fullscreen
shift + alt - f : yabai -m window --toggle native-fullscreen
